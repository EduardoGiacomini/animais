{
  "name": "animais",
  "version": "1.0.0",
  "description": "",
  "main": "script.js",
  "scripts": {
    "devserver": "live-server",
    "watch:scss": "node-sass scss/main.scss styles/style.css -w",
    "compile:scss": "node-sass scss/main.scss styles/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' styles/style.comp.css -o styles/style.prefix.css",
    "compress:css": "node-sass styles/style.prefix.css styles/style.css --output-style compressed",
    "start": "npm-run-all --parallel devserver watch:scss",
    "build": "npm-run-all compile:scss prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EduardoGiacomini/animais.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EduardoGiacomini/animais/issues"
  },
  "homepage": "https://github.com/EduardoGiacomini/animais#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "live-server": "^1.2.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  }
}
